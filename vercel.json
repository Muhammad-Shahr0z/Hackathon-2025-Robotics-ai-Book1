{
  "version": 2,
  "builds": [
    {
      "src": "chatbot-agent/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "chatbot-agent/main.py"
    }
  ],
  "functions": {
    "chatbot-agent/main.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}